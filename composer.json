{
    "name": "yiyon/laravel-permission-merchant",
    "description": "Permission handling for Laravel 5.4 to 5.8,增加多商户权限, 北京易用联友",
    "keywords": [
        "spatie",
        "laravel",
        "permission", "roles", "permissions", "rbac",
        "acl",
        "security"
    ],
    "homepage": "https://github.com/Yiyon/laravel-permission-merchant",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten, linshaoneng",
            "email": "freek@spatie.be, linshaoneng@sina.cn",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.0",
        "illuminate/auth": "~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/container": "~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/contracts": "~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/database": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.4.2|~3.5.0|~3.6.0|~3.7.0",
        "phpunit/phpunit": "^5.7|6.2|^7.0",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Yiyon\\Permission\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Yiyon\\Permission\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yiyon\\Permission\\PermissionServiceProvider"
            ]
        }
    }
}
